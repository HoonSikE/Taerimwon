<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        >
        <!--발주자-->
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layout_result_text_leader"
            android:background="@color/contentBackground"
            android:paddingHorizontal="8dp"
            android:paddingVertical="20dp"
            android:elevation="4dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            >
            <TextView
                android:id="@+id/text_result_title_leader"
                style="@style/SubHeadBold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:text="● 발주자"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                />
            <TextView
                android:id="@+id/text_result_text_leader"
                style="@style/SubHead"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="8dp"
                android:text="발주자 내용"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/text_result_title_leader"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>
        <View
            android:id="@+id/View1"
            android:layout_width="match_parent"
            android:layout_height="10dp"
            android:background="@color/viewColor"
            app:layout_constraintTop_toBottomOf="@+id/layout_result_text_leader"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            tools:ignore="MissingConstraints"
            />
        <!--상주 정보-->
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layout_result_text_client"
            android:background="@color/contentBackground"
            android:paddingHorizontal="8dp"
            android:paddingVertical="20dp"
            android:elevation="4dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/View1"
            >
            <TextView
                android:id="@+id/text_result_title_client"
                style="@style/SubHeadBold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:text="● 상주 정보"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                />
            <TextView
                android:id="@+id/text_result_text_client"
                style="@style/SubHead"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="8dp"
                android:text="상주 정보 내용"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/text_result_title_client"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>
        <View
            android:id="@+id/View2"
            android:layout_width="match_parent"
            android:layout_height="10dp"
            android:background="@color/viewColor"
            app:layout_constraintTop_toBottomOf="@+id/layout_result_text_client"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            tools:ignore="MissingConstraints"
            />
        <!--발주 장소-->
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layout_result_text_location"
            android:background="@color/contentBackground"
            android:paddingHorizontal="8dp"
            android:paddingVertical="20dp"
            android:elevation="4dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/View2"
            >
            <TextView
                android:id="@+id/text_result_title_location"
                style="@style/SubHeadBold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:text="● 발주 장소"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                />
            <TextView
                android:id="@+id/text_result_text_location"
                style="@style/SubHead"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="8dp"
                android:text="발주 장소 내용"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/text_result_title_location"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>
        <View
            android:id="@+id/View3"
            android:layout_width="match_parent"
            android:layout_height="10dp"
            android:background="@color/viewColor"
            app:layout_constraintTop_toBottomOf="@+id/layout_result_text_location"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            tools:ignore="MissingConstraints"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>