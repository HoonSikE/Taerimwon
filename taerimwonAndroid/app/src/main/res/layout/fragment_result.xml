            android:id="@+id/button_result_to_order_fragment2"
            android:layout_width="0dp"
            android:layout_height="48dp"
            app:layout_constraintBottom_toTopOf="@+id/button_MMS"
            app:layout_constraintStart_toStartOf="parent"
