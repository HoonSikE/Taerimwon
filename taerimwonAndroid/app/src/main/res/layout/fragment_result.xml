                            <TextView
                                android:id="@+id/layout_result1_4_2"
                                android:layout_width="20px"
                                android:layout_height="20px"
                                android:text="1"
                                android:textSize="20px"
                                android:textColor="@color/black"
                                android:fontFamily="@font/century"
                                android:textFontWeight="900"
