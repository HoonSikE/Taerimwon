                                app:layout_constraintEnd_toEndOf="parent"
                                android:gravity="center"
                                app:layout_constraintTop_toBottomOf="@+id/layout_result2_1"
                                android:layout_marginTop="5px"
                                android:lineSpacingMultiplier="1.2"
