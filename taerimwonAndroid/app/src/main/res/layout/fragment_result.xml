                                android:layout_width="20px"
                                android:layout_height="20px"
                                android:text="2"
                                android:textSize="20px"
                                android:textColor="@color/black"
                                android:fontFamily="@font/century"
                                android:textFontWeight="900"
                                app:layout_constraintStart_toStartOf="@+id/layout_result3_1"
                                app:layout_constraintEnd_toEndOf="@+id/layout_result3_1"
                                android:gravity="center"
                                app:layout_constraintTop_toBottomOf="@+id/layout_result3_1"
                                />
                            <TextView
                                android:id="@+id/layout_result3_2_2"
                                android:layout_width="20px"
                                android:layout_height="20px"
                                android:text="0"
                                android:textSize="20px"
                                android:textColor="@color/black"
                                android:fontFamily="@font/century"
                                android:textFontWeight="900"
                                app:layout_constraintStart_toStartOf="@+id/layout_result3_1"
                                app:layout_constraintEnd_toEndOf="@+id/layout_result3_1"
                                android:gravity="center"
                                app:layout_constraintTop_toBottomOf="@+id/layout_result3_2_1"
                                />
                            <TextView
                                android:id="@+id/layout_result3_2_3"
                                android:layout_width="20px"
                                android:layout_height="20px"
                                android:text="2"
                                android:textSize="20px"
                                android:textColor="@color/black"
                                android:fontFamily="@font/century"
                                android:textFontWeight="900"
                                app:layout_constraintStart_toStartOf="@+id/layout_result3_1"
                                app:layout_constraintEnd_toEndOf="@+id/layout_result3_1"
                                android:gravity="center"
                                app:layout_constraintTop_toBottomOf="@+id/layout_result3_2_2"
                                />
                            <TextView
                                android:id="@+id/layout_result3_2_4"
                                android:layout_width="20px"
                                android:layout_height="20px"
                                android:text="3"
                                android:textSize="20px"
                                android:textColor="@color/black"
                                android:fontFamily="@font/century"
                                android:textFontWeight="900"
                                app:layout_constraintStart_toStartOf="@+id/layout_result3_1"
                                app:layout_constraintEnd_toEndOf="@+id/layout_result3_1"
                                android:gravity="center"
                                app:layout_constraintTop_toBottomOf="@+id/layout_result3_2_3"
                                />
                            <TextView
                                android:id="@+id/layout_result3_3"
                                android:layout_width="20px"
                                android:layout_height="20px"
                                android:text="•"
                                android:textSize="20px"
                                android:textColor="@color/black"
                                android:fontFamily="@font/century"
                                android:textFontWeight="900"
                                app:layout_constraintStart_toStartOf="@+id/layout_result3_1"
                                app:layout_constraintEnd_toEndOf="@+id/layout_result3_1"
                                android:gravity="center"
                                app:layout_constraintTop_toBottomOf="@+id/layout_result3_2_4"
                                android:layout_marginTop="-8px"
                                />
                            <TextView
                                android:id="@+id/layout_result3_4_1"
                                android:layout_width="20px"
                                android:layout_height="20px"
                                android:text="0"
                                android:textSize="20px"
                                android:textColor="@color/black"
                                android:fontFamily="@font/century"
                                android:textFontWeight="900"
                                app:layout_constraintStart_toStartOf="@+id/layout_result3_1"
                                app:layout_constraintEnd_toEndOf="@+id/layout_result3_1"
                                android:gravity="center"
                                app:layout_constraintTop_toBottomOf="@+id/layout_result3_3"
                                />
                            <TextView
                                android:id="@+id/layout_result3_4_2"
                                android:layout_width="20px"
                                android:layout_height="20px"
                                android:text="9"
                                android:textSize="20px"
                                android:textColor="@color/black"
                                android:fontFamily="@font/century"
                                android:textFontWeight="900"
                                app:layout_constraintStart_toStartOf="@+id/layout_result3_1"
                                app:layout_constraintEnd_toEndOf="@+id/layout_result3_1"
