                                android:textSize="20px"
                                android:textColor="@color/black"
                                android:fontFamily="@font/century"
                                android:textFontWeight="900"
                                app:layout_constraintStart_toStartOf="@+id/layout_result1_1"
                                app:layout_constraintEnd_toEndOf="@+id/layout_result1_1"
                                android:gravity="center"
                                app:layout_constraintTop_toBottomOf="@+id/layout_result1_3"
